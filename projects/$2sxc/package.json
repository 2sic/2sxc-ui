{
  "name": "2sic-2sxc-js-api",
  "version": "14.00.00",
  "description": "The $2sxc JS API for 2sxc.org",
  "scripts": {
    "release": "webpack --mode=production",
    "2sxc-js-prod": "webpack --watch --mode=production",
    "2sxc-js-dev": "webpack --watch --mode=development",

    "// typedoc and docFx versions": "https://github.com/docascode/type2docfx/issues/63",
    "docs": "npm run docs-1-typedoc && npm run docs-2-docFx",
    "docs-1-typedoc": "cd src && delete.cmd \"..\\docs\\typedoc\" && cd .. && typedoc --tsconfig ./src/tsconfig.typedoc.json",
    "docs-2-docFx": "cd src && delete.cmd \"..\\docs\\docFx\" && cd .. && type2docfx ./docs/typedoc/typedoc.json ./docs/docFx",
    "serve-docFx": "cd docs && docfx ./docfx-serve.config.json --serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2sic/2sxc-ui"
  },
  "author": "2sic Internet Solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2sic/2sxc-ui/issues"
  },
  "homepage": "https://github.com/2sic/2sxc-ui",
  "devDependencies": {
    "@types/jquery": "^2.0.33",
    "babel-eslint": "^8.2.3",
    "jshint": "^2.12.0",
    "jshint-stylish": "2.1.0",
    "ts-loader": "^7.0.0",
    "type2docfx": "^0.10.5",
    "typedoc": "^0.15.8",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-shell-plugin": "^0.5.0"
  },
  "jshintConfig": {
    "laxbreak": true
  },
  "dependencies": {
    "natives": "^1.1.6",
    "whatwg-fetch": "^3.0.0"
  }
}
