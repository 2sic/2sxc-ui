{
  "name": "2sic-2sxc-js-api",
  "version": "14.00.00",
  "description": "The $2sxc JS API for 2sxc.org",
  "scripts": {
    "release": "webpack --mode=production",
    "2sxc-js-prod": "webpack --watch --mode=production",
    "2sxc-js-dev": "webpack --watch --mode=development",
    "test": "jasmine-ts",
    "test-browser": "jasmine-browser-runner",
    "test-browser:all": "npm run test-browser:build-src && npm run test-browser:build-spec && jasmine-browser-runner runSpecs",
    "test-browser:serve": "npm run test-browser:build-src && npm run test-browser:build-spec && jasmine-browser-runner",
    "test-browser:build-src": "cross-env NODE_ENV=test npx webpack --config webpack-test.config.js --watch",
    "test-browser:build-spec": "tsc -p ./spec-browser/tsconfig.json --watch",
    "postinstall": "npx json -I -f ./node_modules/jasmine/package.json -e \"delete this.exports\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2sic/2sxc-ui"
  },
  "author": "2sic Internet Solutions",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2sic/2sxc-ui/issues"
  },
  "homepage": "https://github.com/2sic/2sxc-ui",
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "@types/jquery": "^2.0.33",
    "babel-eslint": "^8.2.3",
    "cross-env": "^7.0.3",
    "glob": "^8.0.3",
    "jasmine": "^4.3.0",
    "jasmine-browser-runner": "^1.1.0",
    "jasmine-core": "^4.3.0",
    "jasmine-jquery": "^2.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "jshint": "^2.12.0",
    "jshint-stylish": "2.1.0",
    "path": "^0.12.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^8.10.2",
    "type2docfx": "^0.10.5",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "jshintConfig": {
    "laxbreak": true
  }
}
