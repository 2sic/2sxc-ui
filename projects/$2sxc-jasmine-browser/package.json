{
  "name": "sxc-jasmine-browser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsc -w",
    "build": "npx webpack --config=webpack.config.js",
    "test": "jasmine-browser-runner runSpecs",
    "test:watch": "concurrently -k \"tsc -w\" \"npm run js2mjs:watch\" \"jasmine-browser-runner\"",
    "js2mjs:watch": "nodemon -e js --watch spec --exec \"npm run js2mjs:run\"",
    "js2mjs:run": "npx renamer --force --find /\\.js$/ --replace .mjs --path-element ext \"spec/!(support)**/*.*\" -v"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "@types/jquery": "^3.5.14",
    "concurrently": "^7.4.0",
    "jasmine": "^4.4.0",
    "jasmine-browser-runner": "^1.1.0",
    "jasmine-core": "^4.4.0",
    "jasmine-jquery": "^2.1.1",
    "nodemon": "^2.0.19",
    "path": "^0.12.7",
    "renamer": "^4.0.0",
    "typescript": "^4.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "spec/support/*"
    ],
    "delay": 1000
  }
}
