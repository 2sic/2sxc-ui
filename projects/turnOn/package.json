{
  "name": "turnon-deploy",
  "version": "1.0.0",
  "description": "helper env to deploy turnOn in 2sxc dev env",
  "scripts": {
    "release": "set NODE_ENV=production && node ../webpack/distribute2.js ./node_modules/turn-on/dist/ dist/turnon/",
    "release-watch": "set NODE_ENV=production && node ../webpack/distribute2.js ./node_modules/turn-on/dist/ dist/turnon/ -w",
    "update": "npm install \"github:2sic/turnOn\" -D"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2sic/2sxc-ui"
  },
