{
  "name": "2sxc-quick-dialog",
  "//version-notes": "FYI: This version isn't used for the build.",
  "version": "12.01.00",
  "license": "MIT",
  "scripts": {
    "// notes": "Something isn't quite right with the setup. Basically the dev-dist which should copy to DNN must be restarted after every change :(",
    "dev-angular": "ng build --watch",
    "dev-dist": "node ../webpack/distribute2.js ./dist/quick-dialog dist/quick-dialog/ -w",
    "dev": "concurrently \"ng build --configuration development --watch\" \"node ../webpack/distribute2.js ./dist/quick-dialog/ dist/quick-dialog/ -w\"",
    "release": "set NODE_ENV=production && ng build --configuration production && node ../webpack/distribute2.js ./dist/quick-dialog/ dist/quick-dialog/",
    "release-watch": "set NODE_ENV=production && concurrently \"ng build --configuration production --watch\"  \"node ../webpack/distribute2.js ./dist/quick-dialog dist/quick-dialog/ -w\""
  },
  "private": true,
